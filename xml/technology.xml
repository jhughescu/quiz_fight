<quiz>
  <question>
    <text>What does CPU stand for?</text>
    <options>
      <option>Central Processing Unit</option>
      <option>Computer Personal Unit</option>
      <option>Central Personal Unit</option>
      <option>Computer Processing Unit</option>
    </options>
    <answer>Central Processing Unit</answer>
  </question>
  <question>
    <text>What does HTML stand for?</text>
    <options>
      <option>Hypertext Markup Language</option>
      <option>Hyperlink Markup Language</option>
      <option>Home Tool Markup Language</option>
      <option>Hierarchical Textual Markup Language</option>
    </options>
    <answer>Hypertext Markup Language</answer>
  </question>
  <question>
    <text>What is the smallest unit of digital information?</text>
    <options>
      <option>Bit</option>
      <option>Byte</option>
      <option>Megabyte</option>
      <option>Gigabyte</option>
    </options>
    <answer>Bit</answer>
  </question>
  <question>
    <text>What programming language is known for its use in creating web applications and dynamic websites?</text>
    <options>
      <option>JavaScript</option>
      <option>Python</option>
      <option>C++</option>
      <option>Java</option>
    </options>
    <answer>JavaScript</answer>
  </question>
  <question>
    <text>Which company developed the first commercial microprocessor, the Intel 4004?</text>
    <options>
      <option>Intel</option>
      <option>IBM</option>
      <option>Microsoft</option>
      <option>Apple</option>
    </options>
    <answer>Intel</answer>
  </question>
  <question>
    <text>What does VPN stand for?</text>
    <options>
      <option>Virtual Private Network</option>
      <option>Virtual Public Network</option>
      <option>Very Personal Network</option>
      <option>Visual Processing Node</option>

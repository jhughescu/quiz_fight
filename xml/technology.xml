  <question>
    <text>What is the process of converting analog signals into digital data called?</text>
    <options>
      <option>Analog-to-Digital Conversion</option>
      <option>Digital-to-Analog Conversion</option>
      <option>Analog-Digital Shifting</option>
      <option>Data Digitization</option>
    </options>
    <answer>Analog-to-Digital Conversion</answer>
